<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.model.Product" table="Product_Info">
		<id   name="pid" column="Prod_Id" length="10" ></id>
		<property name="category" column="Category" length="25"/>
		<property name ="price" column="Price" length="10"/>
		<property name ="pname" column = "Prod_Name" length="25"/>
		<property name ="city" column = "City" length="25"/>
	</class>
</hibernate-mapping>